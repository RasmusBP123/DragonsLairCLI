<Window x:Class="GUI.Make_New_Round"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="Make_New_Round" Height="400" Width="400">
    <Grid>
        <TextBox x:Name ="TextInputNewRound" HorizontalAlignment="Left" Height="99" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="163" Margin="111,215,0,0"/>
        <Button x:Name ="ButtonNewRound" Content="Create Round" HorizontalAlignment="Left" VerticalAlignment="Top" Width="161" Height="86" Margin="113,48,0,0" Click="ButtonNewRound_Click"/>
        <Label x:Name ="LabelInMakeNewRound" Content="Tournament Name:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="113,159,0,0" Height="36" Width="161"/>

    </Grid>
</Window>
